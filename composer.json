{
	"name": "icybee/module-files",
	"type": "icanboogie-module",
	"description": "Manages the files uploaded by the users of the CMS Icybee.",
	"keywords": [
		"file",
		"upload"
	],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "http://olvlvl.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/Icybee/module-files/issues",
		"source": "https://github.com/Icybee/module-files"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"prefer-dist": true,
	"require": {
		"php": "^5.6 || ^7.0",
		"icanboogie/module-installer": "^4.0",
		"icybee/core": "^4.0",
		"icybee/module-nodes": "^4.0",
		"icybee/module-registry": "^4.0"
	},
	"require-dev": {
		"icanboogie/icanboogie": "^4.0"
	},
	"autoload": {
		"psr-4": {
			"Icybee\\Modules\\Files\\": "lib"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/Application.php"
		],
		"psr-4": {
			"Icybee\\Modules\\Files\\": "tests/lib"
		}
	},
	"scripts": {
		"post-autoload-dump": "ICanBoogie\\Autoconfig\\Hooks::on_autoload_dump"
	}
}
